{
  "name": "ritika-agrawal811/github-readme-blog-cards",
  "description": "Generate embeddable blog preview cards for GitHub README",
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "require": {},
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "php -S localhost:8000 -t public"
    ],
    "php-lint": "find . -type f -name '*.php' ! -path './vendor/*' -exec php -l {} \\;",
    "format": "npx prettier --write ."
  }
}
